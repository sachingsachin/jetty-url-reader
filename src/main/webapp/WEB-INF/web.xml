<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app 
   xmlns="http://java.sun.com/xml/ns/javaee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
   version="2.5">

   <servlet>
       <servlet-name>UrlReader</servlet-name>
       <servlet-class>example.UrlReaderServlet</servlet-class>
   </servlet>
   <servlet-mapping>
       <servlet-name>UrlReader</servlet-name>
       <url-pattern>/url-reader/*</url-pattern>
   </servlet-mapping>


	<!--*******************-->
	<!--   url-input.jsp   -->
	<!--*******************-->
	<servlet>
	    <servlet-name>url-input.jsp</servlet-name>
	    <jsp-file>/jsp/url-input.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
	    <servlet-name>url-input.jsp</servlet-name>
	    <url-pattern>/url-input</url-pattern>
	</servlet-mapping>

</web-app>
